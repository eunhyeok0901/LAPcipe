<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="ingredient">
	<select id="getIngredient" parameterClass="String" resultClass="ingrevo">
		select * from ingredient where ing_name = #ing_name#
	</select>

</sqlMap>